generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
  moduleFormat = "cjs"
  
}

datasource db {
  provider = "postgresql"
}

model StatusTicket {
  id        Int      @id @default(autoincrement())
  nome      String   @unique
  categoria String
  ativo     Boolean  @default(true)

  criadoEm   DateTime @default(now())
  criadoPor  Int      @default(1)
  alteradoEm DateTime @updatedAt
  alteradoPor Int     @default(1)
  deletadoEm DateTime?
}
